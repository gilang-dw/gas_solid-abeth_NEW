FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.air 1
    volScalarFieldValue alpha.particles 0
);

regions
(
    boxToCell
    {
        box (-0.01 0.03 -0.1) (0.6 0.6 0.1);
        fieldValues
        (
            volScalarFieldValue alpha.air 0.38
            volScalarFieldValue alpha.particles 0.62
        );
    }
);




